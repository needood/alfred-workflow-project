{
  "name": "alfred-things",
  "version": "0.0.0",
  "description": "things",
  "license": "MIT",
  "repository": "needood/alfred-things",
  "author": {
    "name": "needood",
    "email": "tamamaspace@gmail.com",
    "url": "needood.com"
  },
  "pkg": {
    "assets": "templates/**/*"
  },
  "bin": {
    "things": "lib/cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prepublish": "npm run babel-lib",
    "babel-lib": "cross-env BABEL_ENV=lib babel --extensions .ts --root-mode upward --source-maps=true src --out-dir lib",
    "watch": "npm run watch:babel-lib",
    "watch:babel-lib": "cross-env BABEL_ENV=lib babel --extensions .ts --root-mode upward --source-maps=true src --watch --out-dir lib"
  },
  "files": [
    "index.js",
    "icon.png",
    "info.plist"
  ],
  "keywords": [
    "alfred",
    "workflow"
  ],
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "cliver4alfred": "^0.1.0",
    "cliver": "^0.4.1",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "core-js": "3",
    "find-in-files": "^0.5.0",
    "inquirer": "^6.4.1",
    "lru-cache": "^5.1.1",
    "markdown": "^0.5.0",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.29.2",
    "remarkable": "^1.7.1",
    "table": "^5.4.1",
    "tslib": "^1.10.0",
    "v8-compile-cache": "^2.1.0",
    "walk": "^2.3.14",
    "walk-sync": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^12.7.3"
  }
}
